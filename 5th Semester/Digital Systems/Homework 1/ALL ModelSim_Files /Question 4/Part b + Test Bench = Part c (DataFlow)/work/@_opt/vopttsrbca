library verilog;
use verilog.vl_types.all;
entity DataFlowALU is
    port(
        A               : in     vl_logic_vector(7 downto 0);
        B               : in     vl_logic_vector(7 downto 0);
        cin             : in     vl_logic;
        S               : in     vl_logic_vector(3 downto 0);
        Z               : out    vl_logic;
        cout            : out    vl_logic;
        D               : out    vl_logic_vector(7 downto 0)
    );
end DataFlowALU;
